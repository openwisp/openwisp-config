	option path 'platform/ar934x_wmac'
	option disabled '1'
	option country 'IT'
	option phy 'phy0'
	option hwmode '11g'
	option channel '9'
	option type 'mac80211'
	option htmode 'HT20'

config wifi-device 'radio3'
	option hwmode '11a'
	option channel '14'
	option country 'IN'
